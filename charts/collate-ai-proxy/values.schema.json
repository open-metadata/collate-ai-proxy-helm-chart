{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "replicaCount": {
      "type": "integer",
      "minimum": 0
    },
    "image": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "repository": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        },
        "pullPolicy": {
          "type": "string"
        }
      }
    },
    "accountId": {
      "type": "string"
    },
    "region": {
      "type": "string"
    },
    "collate": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "hostAndPort": {
          "type": "string"
        }
      }
    },
    "imagePullSecrets": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "nameOverride": {
      "type": "string"
    },
    "fullnameOverride": {
      "type": "string"
    },
    "commonLabels": {
      "type": "object"
    },
    "podLabels": {
      "type": "object"
    },
    "podAnnotations": {
      "type": "object"
    },
    "serviceAccount": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "create": {
          "type": "boolean"
        },
        "automount": {
          "type": "boolean"
        },
        "annotations": {
          "type": "object"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "podSecurityContext": {
      "type": "object"
    },
    "securityContext": {
      "type": "object"
    },
    "service": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "ClusterIP",
            "NodePort",
            "LoadBalancer"
          ]
        },
        "annotations": {
          "type": "object"
        },
        "ports": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "http": {
              "type": "integer",
              "minimum": 1
            },
            "httpAdmin": {
              "type": "integer",
              "minimum": 1
            },
            "grpc": {
              "type": "integer",
              "minimum": 1
            },
            "metrics": {
              "type": "integer",
              "minimum": 1
            }
          },
          "required": [
            "http",
            "httpAdmin",
            "grpc",
            "metrics"
          ]
        }
      },
      "required": [
        "type",
        "ports"
      ]
    },
    "envFrom": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "extraEnvs": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "tolerations": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "nodeSelector": {
      "type": "object"
    },
    "affinity": {
      "type": "object"
    },
    "resources": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "limits": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "cpu": {
              "type": "string"
            },
            "memory": {
              "type": "string"
            }
          }
        },
        "requests": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "cpu": {
              "type": "string"
            },
            "memory": {
              "type": "string"
            }
          }
        }
      }
    },
    "livenessProbe": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "enabled": {
          "type": "boolean"
        }
      }
    },
    "readinessProbe": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "enabled": {
          "type": "boolean"
        }
      }
    },
    "serviceMonitor": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "interval": {
          "type": "string"
        },
        "annotations": {
          "type": "object"
        },
        "labels": {
          "type": "object"
        }
      }
    },
    "volumes": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "volumeMounts": {
      "type": "array",
      "items": {
        "type": "object"
      }
    }
  }
}
